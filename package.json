    "netlify-build": "netlify-lambda build src/api",
    "serve": "netlify dev"
  },
  "dependencies": {
    "babel-jest": "^24.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "dotenv": "^8.1.0",
    "jest": "^24.8.0",
    "mathjs": "^5.9.0",
    "netlify-cli": "^2.15.0",
    "netlify-lambda": "^1.6.3",
    "twilio": "^3.34.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "devDependencies": {
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "style-loader": "^1.0.0"
  }
