{
  "name": "decabyt.es",
  "version": "1.0.0",
  "author": "Leslie Cheng",
  "repository": "https://github.com/lcfyi/decabyt.es",
  "private": false,
  "scripts": {
    "test": "node scripts/bootstrap.js; jest --verbose; node scripts/cleanup.js",
    "lint": "node scripts/bootstrap.js; eslint --ext .js ./src; node scripts/cleanup.js",
